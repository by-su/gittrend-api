{
  "analysis": {
    "filter": {
      "name_ngram_filter": {
        "type": "edge_ngram",
        "min_gram": 2,
        "max_gram": 20
      },
      "english_stop": {
        "type": "stop",
        "stopwords": "_english_"
      }
    },
    "analyzer": {
      "github_general_analyzer": {
        "char_filter": [],
        "tokenizer": "standard",
        "filter": ["lowercase", "english_stop"]
      },
      "name_ngram_analyzer": {
        "tokenizer": "standard",
        "filter": ["lowercase", "name_ngram_filter"]
      }
    }
  }
}